# Environment Configuration for Brushin.in
# Copy this file to .env.local for local development
# Copy to .env.production for production deployment

# App Configuration
VITE_APP_NAME=Brushin.in
VITE_APP_VERSION=3.0.0
VITE_APP_ENV=development

# API Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# External Services
VITE_OPENAI_API_KEY=your_openai_api_key_here
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# Analytics and Monitoring
VITE_GOOGLE_ANALYTICS_ID=your_ga_id_here
VITE_GOOGLE_TAG_MANAGER_ID=your_gtm_id_here

# Social Media
VITE_TWITTER_HANDLE=@BrushinAI
VITE_LINKEDIN_URL=https://linkedin.com/company/brushin

# Feature Flags
VITE_ENABLE_ANALYTICS=true
VITE_ENABLE_PWA=true
VITE_ENABLE_OFFLINE_MODE=true

# Performance
VITE_ENABLE_LAZY_LOADING=true
VITE_ENABLE_IMAGE_OPTIMIZATION=true
VITE_ENABLE_CODE_SPLITTING=true
